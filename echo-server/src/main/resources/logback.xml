<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>
<!--              %d{yyyy-MM-dd HH:mm:ss.SSS} %-5level [%10.10t] %-35.35logger{34} [%X{CALL_ID},%X{traceId},%X{spanId}]: %msg%n-->
              %d{yyyy-MM-dd HH:mm:ss.SSS} %-5level [%10.10t] %-35.35logger{34} [%X{CALL_ID},%X{transaction.id},%X{trace.id},%X{span.id}]: %msg%n
            </pattern>
        </encoder>
    </appender>
    <root level="info">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
